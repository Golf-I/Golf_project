<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd"><html><head>	<meta charset="UTF-8">    <meta http-equiv="X-UA-Compatible" content="IE=edge">    <meta name="viewport" content="width=device-width, initial-scale=1.0">    <title>join02</title>    <link href="${pageContext.request.contextPath}/resources/css/join02.css" rel="stylesheet">    <script	src="${pageContext.request.contextPath}/resources/js/member_signup2.js"></script></head><body>	<section class="join">        <div class="join_box">            <div class="join_title">                <p>회원가입</p>             </div><!--title-->			<form action="member/signup" method="post" name="join" onsubmit="return join2_check();">			 <!-- enctype="multipart/form-data" -->             <div class="confirm_box">                <p>회원님의 명의로 등록된 휴대폰으로 본인확인을 진행합니다.</p>				                <input type="hidden" value="${vo.name}" name="name">                <input type="hidden" value="${vo.id}" name="id">                <input type="hidden" value="${vo.pw}" name="pw">                <input type="hidden" value="${vo.icon}" name="icon">                <input type="hidden" value="${vo.sns}" name="sns">                <div class="phone">                    <select>                        <option>대한민국</option>                        <option>　</option>                    </select>                    <input type="text" size="18" maxlength="18" required placeholder="휴대폰번호(숫자만입력)" id="phone_n" name="phone">                </div>                <div class="certification_number">                    <input type="text" size="18" maxlength="18" required placeholder="인증번호입력" id="certification">                    <a href="#">                        <p>인증번호받기</p>                    </a>                </div>            </div><!--confirm_box-->            <div class="check_box">            	            <div class="all_chk">	            	<label for="all_chk">	                	<input type="checkbox" id="all_chk" > 전체 동의하기	            	</label>	            </div>					<label for="e-mail_chk">		            	<input type="checkbox" id="e-mail_chk" name="mail" class="checkbox"> 이메일로 소식받기 <br>					</label>					<label for="message_chk">			            <input type="checkbox" id="message_chk" name="mms" class="checkbox"> 문자메시지로 소식받기					</label>	            <div class="agree01">					<label for="agree01">	                <input type="checkbox" id="agree01" class="checkbox"><span id="agree01false"><b> 이용약관 동의하기</b></span>	                <a href="javascript:open('member_term_of_service', '이용약관', 'width=1350, height=1000');"><p>>>상세보기</p></a>					</label>	            </div><!--동의-->            <div class="agree02">            	<label for="agree02">                <input type="checkbox" id="agree02" class="checkbox"><span id="agree02false"><b> 개인정보취급방침 동의하기</b></span>                <a href="javascript:open('member_privacy_policy', '개인정보취급방침', 'width=1350, height=1000');"><p>>>상세보기</p></a>                </label>            </div><!--동의-->            </div><!--chk_box-->           	<input type="submit" value="확인" id="confirm" class="join_Btn"/>                        </form>        </div><!--box-->    </section><!--join--></body></html>